{
  "rules": {
    "align": [true,
        "parameters",
        "arguments",
        "statements"
    ],
    "no-construct": true,
    "no-debugger": true,
    "no-duplicate-key": true,
    "no-duplicate-variable": true,
    "no-empty": true,
    "no-eval": true,
    "no-string-literal": true,
    "no-switch-case-fall-through": true,
    "no-trailing-comma": true,
    "no-unreachable": true,
    "no-var-keyword": true,
    "no-var-requires": true
  }
}
